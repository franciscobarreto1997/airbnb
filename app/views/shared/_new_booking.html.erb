<%= simple_form_for [@flat, @booking] do |f| %>
  <%= f.input :start_date, as: :string, required: true, label: "CHECK-IN", placeholder: "dd/mm/yyyy", input_html: {id: "start_date"} %>
  <%= f.input :end_date, as: :string, required: true, label: "CHECK-OUT", placeholder: "dd/mm/yyyy", disabled: true, input_html: {id: "end_date"} %>
<% end %>

